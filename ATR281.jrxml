	<queryString language="SQL">
		<![CDATA[SELECT TH_REF_ID,TH_TRAINING_TITLE,TH_ORGANIZER_NAME,
TO_CHAR(TH_DATE_FROM,'dd/mm/yyyy')||' hingga '||TO_CHAR(TH_DATE_TO,'dd/mm/yyyy') DATE_DURATION,
TO_CHAR(TH_TRAINING_FEE,'999,999,999.99') FEE,
TH_TRAINING_VENUE||' , '||get_state_name(TH_TRAINING_STATE)||' , '||get_country_name(TH_TRAINING_COUNTRY) VENUE,
sm_staff_name
FROM TRAINING_HEAD, staff_training_head,staff_main,staff_service,staff_job_status
WHERE TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
and sth_training_refid = TH_REF_ID
and sth_staff_id = sm_staff_id
and ss_staff_id = sm_staff_id
and ss_job_status = sjs_status_code
and sth_staff_id = $P{staffID} 
AND TH_REF_ID =  $P{training_refid} ]]>
	</queryString>
